[user]
        name = Bram Harmsen
        email = bramharmsen@gmail.com
[gui]
        editor = gedit
[credential]
        helper = cache
[core]
        editor = nano
	excludesfile = /home/bremme/.gitignore_global
[color]
        diff = auto 
        ui = auto
        status = auto
        branch = auto
        diff = auto 
[alias]
        st = status
        cm = commit
        cma = commit -a
        pu = push        
        ls = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate
        ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
        alias = "!git config -l | grep alias | cut -c 7-"
		logtree = log --graph --oneline --decorate --all
